@startuml
!include Style.puml
'https://plantuml.com/sequence-diagram

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant "u:PutCommand" as PutCommand LOGIC_COLOR
end box

box Model
participant ":Model" as Model
participant ":AddressBook" as AddressBook
end box

[-> LogicManager : execute(put)
activate LogicManager

LogicManager -> AddressBookParser : parseCommand(put)
activate AddressBookParser

create PutCommand
AddressBookParser -> PutCommand
activate PutCommand

PutCommand --> AddressBookParser
deactivate PutCommand

AddressBookParser --> LogicManager : u
deactivate AddressBookParser

LogicManager -> PutCommand : execute()
activate PutCommand

PutCommand -> Model : putPersonIntoLineup()
activate Model

Model -> AddressBook : addPersonToLineup()
activate AddressBook

AddressBook --> Model
deactivate AddressBook

Model --> PutCommand
deactivate Model

PutCommand --> LogicManager : result
deactivate PutCommand

@enduml