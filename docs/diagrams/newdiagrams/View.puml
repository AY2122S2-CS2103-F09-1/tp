@startuml
!include Style.puml
box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":ViewCommandParser" as ViewCommandParser LOGIC_COLOR
participant "v:ViewCommand" as ViewCommand LOGIC_COLOR
end box

box Model
participant ":Model" as Model LOGIC_COLOR_T4
participant ":AddressBook" as AddressBook LOGIC_COLOR_T4
end box

[-> LogicManager : execute(view)
activate LogicManager

LogicManager -> ViewCommandParser : parseCommand(view)
activate ViewCommandParser

create ViewCommand
ViewCommandParser -> ViewCommand
activate ViewCommand

ViewCommand --> ViewCommandParser
deactivate ViewCommand

ViewCommandParser --> LogicManager : v
deactivate ViewCommandParser

LogicManager -> ViewCommand : execute(model)
activate ViewCommand

ViewCommand -> Model : ViewLineup()
activate Model

Model -> AddressBook : ViewLineup()
activate AddressBook

AddressBook --> Model
deactivate AddressBook

Model --> ViewCommand
deactivate Model

ViewCommand --> LogicManager : result
deactivate ViewCommand
@enduml
